   <children>
      <Label layoutX="48.0" layoutY="46.0" text="New Category:" />
      <TextField fx:id="categoryTextField" layoutX="141.0" layoutY="42.0" prefHeight="25.0" prefWidth="227.0" />
      <Button fx:id="cancelCategory" layoutX="312.0" layoutY="83.0" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
      <Button fx:id="saveCategory" layoutX="240.0" layoutY="83.0" mnemonicParsing="false" onAction="#handleSaveCategory" text="Save" />
   </children>
</AnchorPane>
